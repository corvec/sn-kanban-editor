{
  "name": "sn-kanban-editor",
  "version": "0.3.0",
  "description": "",
  "keywords": [
    "Standard Notes",
    "Standard Notes Extensions",
    "Standard Notes Editors",
    "Kanban Boards",
    "Personal Kanban Board"
  ],
  "private": true,
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/corvec/sn-kanban-editor.git"
  },
  "bugs": {
    "url": "https://github.com/corvec/sn-kanban-editor/issues"
  },
  "sn": {
    "main": "build/index.html"
  },
  "homepage": ".",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean-deploy-cache": "rm -rf node_modules/.cache/gh-pages",
    "predeploy": "npm run build && npm run clean-deploy-cache",
    "deploy-stable": "gh-pages -b gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s -p 3000 build",
    "server-public": "http-server -p 3000 --cors",
    "server-root": "http-server ./ -p 3000 --cors",
    "server": "http-server ./build -p 3000 --cors",
    "pretty": "prettier --write 'src/**/*.{html,css,scss,js,jsx,ts,tsx,json}' README.md"
  },
  "dependencies": {
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "filesafe-embed": "^1.0.10",
    "filesafe-js": "^1.0.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "16",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.2",
    "react-trello": "file:.yalc/react-trello",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "sn-editor-kit": "1.0.6",
    "sn-stylekit": "2.1.0",
    "source-map-explorer": "^2.5.1",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "README.md": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "http-server": "^0.12.3"
  }
}
